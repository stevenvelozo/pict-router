{
	"name": "routed_app",
	"version": "1.0.0",
	"description": "Multi-page SPA example for pict-router with parameterized routes",
	"main": "Routed-App-Application.js",
	"scripts":
	{
		"start": "node Routed-App-Application.js",
		"build": "npx quack build && npx quack copy"
	},
	"author": "steven velozo <steven@velozo.com>",
	"license": "MIT",
	"copyFilesSettings":
	{
		"whenFileExists": "overwrite"
	},
	"copyFiles":
	[
		{ "from": "./html/*", "to": "./dist/" },
		{ "from": "../../node_modules/pict/dist/*", "to": "./dist/js/" }
	],
	"dependencies":
	{
		"pict": "^1.0.293",
		"pict-application": "^1.0.19",
		"pict-router": "^1.0.4",
		"pict-view": "^1.0.63"
	}
}
